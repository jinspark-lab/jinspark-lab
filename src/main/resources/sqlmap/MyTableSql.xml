<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MyTableSql">
    <select id="selectMyTableList" resultType="com.mainlab.model.MyTable">
        SELECT
        `id` as `id`,
        `name` as `name`
        FROM
        `mytable`
    </select>
</mapper>