<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true"
         monitoring="autodetect"
         dynamicConfig="true">

    <cache name="testUserProfile" maxEntriesLocalHeap="1000" eternal="false" overflowToDisk="false"
           diskPersistent="false" timeToIdleSeconds="0" timeToLiveSeconds="3600" memoryStoreEvictionPolicy="LRU"/>
    <cache name="secretsManagerParam" maxEntriesLocalHeap="1000" eternal="false" overflowToDisk="false"
           diskPersistent="false" timeToIdleSeconds="0" timeToLiveSeconds="3600" memoryStoreEvictionPolicy="LRU"/>

</ehcache>